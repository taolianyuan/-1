.refresh-container {
	box-sizing: border-box;
	font-size: 28rpx;
	/*color: #ff5173;*/
	background-color: #000;
	display: flex;
	width: 100%;
}

.refresh-layout {
	vertical-align: center;
	width: 100%;
	text-align: center;
	align-self: flex-end;
}

.refresh-loading {
	margin: 0 5px;
	width: 30rpx;
	height: 30rpx;
	display: inline-block;
	vertical-align: middle;
	-webkit-animation: weuiLoading 1s steps(12, end) infinite;
	animation: weuiLoading 1s steps(12, end) infinite;
	background: transparent url(data:image/svg+xml;base64,) no-repeat;
	background-size: 100%;
	color: #ff5173;
}

.refresh-tips {
	line-height: 30rpx;
	font-size: 26rpx;
