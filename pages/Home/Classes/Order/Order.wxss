.bg{
  margin-bottom: 120rpx;
}

.modal {
  width: 710rpx;
  height: auto;
  margin: 20rpx;
  background-color: #fff;
  border-radius: 16rpx;
}


.title {
  margin-left: 31rpx;
  /* margin-top: 33rpx; */
  font-size: 33rpx;
  font-weight: 600;
  line-height: 80rpx;
  color: rgba(51, 51, 51, 1);
}

.class-bg {
  height: 180rpx;
  width: 100%;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}

.class-item {
  width: 157rpx;
  height: 122rpx;
  margin-left: 16rpx;
  vertical-align: middle;
}

.cell {
  width: 670rpx;
  height: 100rpx;
  margin-left: 20rpx;
  /* border-top: 1px solid #EFEFEF; */
  display: flex;
  align-items: center;
}

.cell-key {
  font-size: 30rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #666;
}

.cell-value {
  position: absolute;
  right: 40rpx;
  height: 100rpx;
  width: auto;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  color: #666;
  font-size: 28rpx;
}

.line {
  width: 670rpx;
  height: 1px;
  background-color: #efefef;
  margin-left: 20rpx;
}

.bottom-bg {
  width: 745rpx;
  height: 115rpx;
  background: rgba(248, 248, 248, 1);
  border: 1px solid rgba(234, 232, 232, 1);
  /* position: absolute; */
  bottom: 0;
  left: 0;
  display: flex;
  position: fixed
}


.price {
  width: 477rpx;
  height: 100%;
  line-height: 114rpx;
  text-align: center;
}

.text-title {
  color: #333;
  font-size: 28rpx;
  font-weight: 600;
}

.text-price {
  color: #ec5a45;
  font-size: 50rpx;
  font-weight: 600;
}

.text-off {
  color: #999;
  font-size: 21rpx;
  font-weight: 600;
}

.submit-order {
  width: 273rpx;
  height: 114rpx;
  background-color: #e27928;
  font-size: 53rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 114rpx;
  text-align: center;
}

.submit-order.des {
  background-color: #bbb;
}

page {
