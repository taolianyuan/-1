  width: 100rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #333;
  font-weight: 600;
  border-top: 1px solid #bbb;
  border-bottom: 1px solid #bbb;
}

.add-minus {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
