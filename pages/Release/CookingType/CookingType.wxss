

.modal{width: 690rpx; height: auto; background-color: #fff; margin: 30rpx; border-radius: 15rpx; padding: 30rpx 0; box-shadow: 0px 0px 20rpx rgba(0, 0, 0, 0.5);
  -webkit-box-shadow:0 6px 11px 2px #00000010;/*for IOS*/}

.title{width: 660rpx; height: 100rpx; margin-left: 30rpx; line-height: 100rpx; font-size: 30rpx; color: #ff5173; font-weight: bold;}

.item-bg{width: 660rpx; height: auto; display: flex; display: flex; flex-wrap: wrap;}

.item{flex-grow: 1; height: auto; padding: 10rpx 20rpx; font-size: 28rpx; background-color: #f5f5f5; border-radius: 6rpx; margin: 30rpx 0 0 30rpx; text-align: center;}

.item.isChecked{background-color: #fff; border: 1px solid #ff5173; padding:8rpx 18rpx;}





.btn-bg{
	height: 10vh;
	width: 100%;
	background-color: #fff;
	justify-content: center; 
	align-items: center; 
	display: flex; 
}

.first-page-btn{width: 80%; 
	height: 90rpx;
	width: 550rpx; 
	justify-content: center; 
	align-items: center; 
	display: flex; 
	border-radius: 10rpx; 
	color: white;
 	background: #ff5173;
 }

























