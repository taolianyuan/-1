.header{width: 100%; height: 400rpx; background: #a31b38;}

.header-icon{width: 200rpx; height: 200rpx; border-radius: 50%; margin-left: 275rpx; margin-top: 50rpx;}

.header-tips{width: 100%; height: auto; margin-top: 20rpx; font-size: 30rpx; color: #fff; text-align: center;}

.module-bg{width: 690rpx; height: auto; margin-left: 30rpx; position: absolute; top:350rpx;}

.module{width: 100%; height: auto; margin-bottom: 100rpx; border-radius: 10rpx;  box-shadow:0 0 0 2rpx #ffb12a inset;/*for Android*/
  -webkit-box-shadow:0 6px 11px 2px #00000010;/*for IOS*/ background-color: #fff; z-index: 1000; }

.xinzuo-icon{width: 50rpx; height: 50rpx; vertical-align: middle;}



.cell{width: 630rpx; height: 100rpx; margin-left: 30rpx; background: #fff; display: flex; align-items: center;}

.cell-key{font-size: 30rpx; width: 80rpx; color: #666;}

.cell-key.title{color: #333; width: auto; font-size: 32rpx; font-weight: 500;}

.cell-key.l{width: 150rpx;}

.cell-value{font-size: 30rpx; color: #666;}

.cell-value.right{ text-align:right; font-size: 26rpx; color: #999; padding-right:30rpx; width: 400rpx; }

.input{width: 566rpx; height: 100rpx; background-color:#F7F7F7; border-radius: 8rpx; font-size: 32rpx; text-align: center;}

.picker{width: 750rpx; height: auto;}

.xinzuo{display: flex; margin-left: 20rpx; align-items: center; justify-content: center;}

.btn{width: 690rpx; height: 100rpx; margin-top: 24rpx; margin-bottom: 26rpx; color: #fff; background-color: #fc5375; border-radius: 10rpx; line-height: 100rpx; text-align: center}

.picker-bg{width: 100%; height: 100; display: flex; align-items: center;}


.radio-group {
  display: flex;
  align-items: center;
 
}
.lable{ margin-right: 20rpx; display: flex; justify-content: center; align-items: center;}

.radio{ font-size: 30rpx; color: #666; margin-top: 5rpx;}

.module-bottom{margin-bottom: 0;}
